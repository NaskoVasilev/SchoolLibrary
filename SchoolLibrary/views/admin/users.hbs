<section>
    <div id="taken-books">
        <table>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Username</th>
                <th>Class</th>
                <th>Number in class</th>
            </tr>
            {{#each users}}
                <tr>
                    <td>{{firstName}}</td>
                    <td>{{lastName}}</td>
                    <td>{{username}}</td>
                    <td>{{class}}</td>
                    <td>{{numberInClass}}</td>
                </tr>
            {{/each}}
        </table>
    </div>
</section>